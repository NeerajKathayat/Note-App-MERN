{
    "version": 2,
    "name": "note",
    "builds": [
        {
            "src": "app.jsx",
            "use": "@vercel/node",
            "config": {
                "maxLambdaSize": "50mb",
                "includeFiles": ["*.jsx"]
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/app.jsx"
        }
    ]
}
